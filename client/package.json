{
	"name": "haskero",
	"description": "VSCode part of haskell intero language server",
	"author": "Julien Vannesson",
	"license": "MIT",
	"version": "0.1.0",
	"publisher": "Vans",
	"engines": {
		"vscode": "^1.8.0"
	},
	"categories": [
		"Languages"
	],
	"activationEvents": [
		"onLanguage:haskell"
	],
	"main": "./out/src/extension",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"update-vscode": "node ./node_modules/vscode/bin/install",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.1.4",
		"@types/mocha": "^2.2.33",
		"@types/node": "^6.0.40",
		"mocha": "^2.3.3",
		"vscode": "^1.0.3"
	},
	"dependencies": {
		"vscode-languageclient": "^3.0.3"
	}
}