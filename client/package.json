{
    "name": "haskero",
    "displayName": "Haskero",
    "description": "Haskell language server for VSCode (errors, completion, goto def, etc.)",
    "author": "Julien Vannesson",
    "license": "MIT",
    "version": "0.1.36",
    "publisher": "Vans",
    "homepage": "https://gitlab.com/vannnns/haskero/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/vannnns/haskero"
    },
    "icon": "images/haskero_128px.png",
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Languages"
    ],
    "extensionDependencies": [
        "justusadam.language-haskell"
    ],
    "activationEvents": [
        "onLanguage:haskell"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "update-vscode": "node ./node_modules/vscode/bin/install",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.1.4",
        "@types/mocha": "^2.2.33",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "vscode": "^1.0.3"
    },
    "dependencies": {
        "vscode-languageclient": "^3.0.3"
    }
}