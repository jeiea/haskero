	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"dependencies": {
		"vscode-languageserver": "^3.0.4",
		"node-uuid": "^1.4.7"
	},
	"devDependencies": {
		"@types/node": "^6.0.52",
		"typescript": "^2.1.4"
	},
	"scripts": {
		"compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
